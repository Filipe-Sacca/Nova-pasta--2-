# Configurações do Supabase
SUPABASE_URL=https://seu-projeto.supabase.co
SUPABASE_KEY=sua-chave-de-api-aqui

# Configurações da API do iFood
IFOOD_API_BASE_URL=https://merchant-api.ifood.com.br
IFOOD_API_TIMEOUT=30
IFOOD_API_RETRY_ATTEMPTS=3

# Configurações do Scheduler
# Intervalo de sincronização em minutos (padrão: 5 minutos como no N8N)
SYNC_INTERVAL_MINUTES=5

# Configurações de processamento
BATCH_SIZE=100
MAX_CONCURRENT_MERCHANTS=5

# Configurações de logging
LOG_LEVEL=INFO
LOG_FILE=ifood_sync.log

# Configurações de performance
ENABLE_CACHING=true
CACHE_TTL_SECONDS=300

# Configurações de modo de execução
DRY_RUN=false
DEBUG_MODE=false